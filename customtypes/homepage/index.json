{
  "format": "page",
  "id": "homepage",
  "label": "Homepage",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "main_text": {
        "type": "StructuredText",
        "config": {
          "label": "Main Text",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,hyperlink"
        }
      },
      "links": {
        "type": "Group",
        "config": {
          "label": "Links",
          "repeat": true,
          "fields": {
            "label": {
              "type": "Text",
              "config": {
                "label": "Label",
                "placeholder": ""
              }
            },
            "link": {
              "type": "Link",
              "config": {
                "label": "Link",
                "placeholder": "",
                "select": null,
                "allowTargetBlank": true,
                "allowText": false,
                "repeat": false
              }
            }
          }
        }
      },
      "yapping": {
        "type": "StructuredText",
        "config": {
          "label": "Yapping",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,em,strong,embed",
          "labels": [
            "strikethrough",
            "underline"
          ]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
